{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1680071865.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"oci-tenancy-based-policy-document"
	],
	"properties": {
		"oci-tenancy-based-policy-document": {
			"$id": "#root/oci-tenancy-based-policy-document", 
			"title": "Oci-tenancy-based-policy-document", 
			"type": "object",
			"required": [
				"name",
				"description",
				"version",
				"policy"
			],
			"properties": {
				"name": {
					"$id": "#root/oci-tenancy-based-policy-document/name", 
					"title": "Name", 
					"type": "string",
					"default": "",
					"examples": [
						"Tenancy-OCI-Administrator-Policies"
					],
					"pattern": "^.*$"
				},
				"description": {
					"$id": "#root/oci-tenancy-based-policy-document/description", 
					"title": "Description", 
					"type": "string",
					"default": "",
					"examples": [
						"The policy set defined for role OCI-Administrator at tenancy level for different groups"
					],
					"pattern": "^.*$"
				},
				"version": {
					"$id": "#root/oci-tenancy-based-policy-document/version", 
					"title": "Version", 
					"type": "string",
					"default": "",
					"examples": [
						"1.0.0"
					],
					"pattern": "^.*$"
				},
				"policy": {
					"$id": "#root/oci-tenancy-based-policy-document/policy", 
					"title": "Policy", 
					"type": "array",
					"default": [],
					"items":{
						"$id": "#root/oci-tenancy-based-policy-document/policy/items", 
						"title": "Items", 
						"type": "string",
						"default": "",
						"examples": [
							"Allow group sandbox to use tag-namespaces in tenancy"
						],
						"pattern": "^.*$"
					}
				}
			}
		}

	}
}
